{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "description": "React boilerplate with React, Redux, Immutable, Babel, ES6 and Webpack.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack",
    "build:stats": "webpack --profile --json > stats.json",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js 'src/**/*.specs.@(js|jsx)'",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "storybook": "start-storybook -p 3001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adambrgmn/react-boilerplate.git"
  },
  "keywords": [
    "ReactJS",
    "Babel",
    "Webpack",
    "ES6",
    "ES2015",
    "Redux",
    "Immutable"
  ],
  "author": "Adam Bergman <adam@fransvilhelm.com> (http://www.fransvilhelm.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adambrgmn/react-boilerplate/issues"
  },
  "homepage": "https://github.com/adambrgmn/react-boilerplate#readme",
  "devDependencies": {
    "@kadira/storybook": "^1.19.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.16.0",
    "html-webpack-template": "^4.0.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jsdom": "^8.4.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.12.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^2.4.2",
    "immutable": "^3.8.1",
    "react": "^15.0.1",
    "react-addons-pure-render-mixin": "^15.0.1",
    "react-dom": "^15.0.1",
    "redux": "^3.4.0"
  }
}
